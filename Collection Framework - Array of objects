package com.pritpal.jdbc;

//Array of objects

//Class whose array of objects will be created
class Student
{
    public int roll_no;
    public String name;
    Student(int roll_no, String name)
    {
        this.roll_no = roll_no;
        this.name = name;
    }
}

public class trySamples {

	public static void main(String[] args) {
		//array of objects/class Student which has 5 elements/student records
		Student[] arr;
		arr = new Student[5];
		arr[0] = new Student(1,"Pritpal");
		arr[1] = new Student(2,"Ajay");
		arr[2] = new Student(3,"Ashish");
		arr[3] = new Student(4,"Neeraj");
		arr[4] = new Student(5,"Sanjeev");
		
		for (int i=0;i < arr.length; i++){
			System.out.println(arr[i].name + " " + arr[i].roll_no);
		}

	}

}
